<%= search_form_for @q, :url => search_static_pages_path,
                        :html => {:method => :post} do |s| %>
  <div class="field">
    <%= s.label :c1_or_c2_or_c3_or_c4_or_c5_or_c6_eq, "Subject" %>
    <%= s.text_field :c1_or_c2_or_c3_or_c4_or_c5_or_c6_eq %>
    <%= s.label :c2_or_c3_or_c4_or_c5_or_c6_or_c1_eq, "Skail 1" %>
    <%= s.text_field :c2_or_c3_or_c4_or_c5_or_c6_or_c1_eq %>
    <%= s.label :c3_or_c4_or_c5_or_c6_or_c1_or_c2_eq, "Skail 2" %>
    <%= s.text_field :c3_or_c4_or_c5_or_c6_or_c1_or_c2_eq %>
    <%= s.label :c4_or_c5_or_c6_or_c1_or_c2_or_c3_eq, "Skail 3" %>
    <%= s.text_field :c4_or_c5_or_c6_or_c1_or_c2_or_c3_eq %>
    <%= s.label :c5_or_c6_or_c1_or_c2_or_c3_or_c4_eq, "Skail 4" %>
    <%= s.text_field :c5_or_c6_or_c1_or_c2_or_c3_or_c4_eq %>
    <%= s.label :c6_or_c1_or_c2_or_c3_or_c4_or_c5_eq, "Skail 5" %>
    <%= s.text_field :c6_or_c1_or_c2_or_c3_or_c4_or_c5_eq %>
  </div>
  <%= s.submit "Search", class: "btn btn-large btn-primary" %>
<% end %>